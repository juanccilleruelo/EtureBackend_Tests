{
  "swagger":"2.0",
  "info":
  {
    "version":"Ver 1.0",
    "title":"Eture Server API",
    "description":"Eture Server RESTful API Documentation",
    "contact":
    {
      "name":"Juan C.Cilleruelo",
      "email":"juanc.cilleruelo@sencille.es",
      "url":"https:\/\/github.com\/juanccilleruelo"
    }
  }
,
  "host":"127.0.0.1:8181",
  "basePath":"\/api",
  "schemes":
  [
    "http"
  ]
,
  "paths":
  {
    "\/login":
    {
      "post":
      {
        "tags":
        [
          "JWT Authentication"
        ]
,
        "description":"Create JSON Web Token",
        "produces":
        [
          "application\/json"
        ]
,
        "parameters":
        [
                    {
            "in":"header",
            "name":"jwtusername",
            "type":"string"
          }
,
          
          {
            "in":"header",
            "name":"jwtpassword",
            "type":"string"
          }
        ]
,
        "responses":
        {
          "200":
          {
            "description":"OK",
            "schema":
            {
              "type":"object",
              "properties":
              {
                "token":
                {
                  "type":"string",
                  "description":"JWT Token"
                }
              }
            }
          }
,
          "401":
          {
            "description":"Invalid authorization type"
          }
,
          "403":
          {
            "description":"Forbidden"
          }
,
          "500":
          {
            "description":"Internal server error"
          }
        }
,
        "security":
        [
                    {
            "basic":
            [
                          ]
          }
        ]
      }
    }
,
    "\/schoolcourses\/getnextschoolcourse":
    {
      "post":
      {
        "tags":
        [
          "SchoolCourses"
        ]
,
        "description":"Get the next school course in sequence",
        "operationId":"SchoolCoursesController.TSchoolCoursesController_GetNextSchoolCourse",
        "consumes":
        [
          "application\/json"
        ]
,
        "produces":
        [
          "application\/json"
        ]
,
        "parameters":
        [
                    {
            "in":"body",
            "name":"CD_SCHOOL_COURSE",
            "description":"Current school course code",
            "required":true,
            "type":"string"
          }
,
          
          {
            "in":"body",
            "name":"Language",
            "description":"Language code (EN\/ES, default EN)",
            "default":"EN",
            "type":"string"
          }
        ]
,
        "responses":
        {
          "200":
          {
            "description":"Next school course with CD_SCHOOL_COURSE based on ORDER_INDEX"
          }
,
          "204":
          {
            "description":"No next course found (last course in sequence)"
          }
,
          "400":
          {
            "description":"Missing required parameter CD_SCHOOL_COURSE"
          }
,
          "500":
          {
            "description":"Internal server error"
          }
        }
      }
    }
,
    "\/schoolcourses\/getone":
    {
      "post":
      {
        "tags":
        [
          "SchoolCourses"
        ]
,
        "description":"Get a specific school course by code",
        "operationId":"SchoolCoursesController.TSchoolCoursesController_GetOne",
        "consumes":
        [
          "application\/json"
        ]
,
        "produces":
        [
          "application\/json"
        ]
,
        "parameters":
        [
                    {
            "in":"body",
            "name":"CD_SCHOOL_COURSE",
            "description":"School course code identifier",
            "required":true,
            "type":"string"
          }
,
          
          {
            "in":"body",
            "name":"Language",
            "description":"Language code (EN\/ES, default EN)",
            "default":"EN",
            "type":"string"
          }
        ]
,
        "responses":
        {
          "200":
          {
            "description":"School course object with CD_SCHOOL_COURSE, DS_SCHOOL_COURSE"
          }
,
          "204":
          {
            "description":"School course not found"
          }
,
          "400":
          {
            "description":"Missing required parameter CD_SCHOOL_COURSE"
          }
,
          "500":
          {
            "description":"Internal server error"
          }
        }
      }
    }
,
    "\/schoolcourses\/getall":
    {
      "post":
      {
        "tags":
        [
          "SchoolCourses"
        ]
,
        "description":"Get all school courses ordered by index",
        "operationId":"SchoolCoursesController.TSchoolCoursesController_GetAll",
        "consumes":
        [
          "application\/json"
        ]
,
        "produces":
        [
          "application\/json"
        ]
,
        "parameters":
        [
                    {
            "in":"body",
            "name":"Language",
            "description":"Language code (EN\/ES, default EN)",
            "default":"EN",
            "type":"string"
          }
        ]
,
        "responses":
        {
          "200":
          {
            "description":"Array of school courses with CD_SCHOOL_COURSE, DS_SCHOOL_COURSE ordered by ORDER_INDEX"
          }
,
          "400":
          {
            "description":"Invalid parameters"
          }
,
          "500":
          {
            "description":"Internal server error"
          }
        }
      }
    }
  }
,
  "securityDefinitions":
  {
    "basic":
    {
      "type":"basic"
    }
,
    "bearer":
    {
      "type":"apiKey",
      "description":"Method for authentication using JSON Web Token (JWT)",
      "in":"header",
      "name":"Authorization"
    }
  }
}
